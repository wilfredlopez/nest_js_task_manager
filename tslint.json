{
  "defaultSeverity": "warn",
  "extends": ["tslint:recommended"],
  "jsRules": {
    "no-unused-expression": true
  },
  "rules": {
    "quotemark": false,
    "semicolon": false,
    "member-access": [false],
    "ordered-imports": [false],
    "max-line-length": [true, 150],
    "member-ordering": [false],
    "interface-name": [false],
    "arrow-parens": false,
    "no-console": false,
    "object-literal-sort-keys": false
  },
  "rulesDirectory": []
}
